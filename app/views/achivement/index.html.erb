  <div class="site">
    <!-- Site Header -->
    <header class="site-header classic relative">
       <%= render 'letter_spring/header' %>
    </header>
    <!--/ Site Header -->
    <main class="site-main">
       <!-- Page header -->
       <div class="page-header page-header-sm">
          <h1 class="text-uppercase page-title">Thành tích đạt được</h1> </div>
       <!-- /Page header -->
       <!-- Breadcrumbs -->
       <div class="breadcrumb">
          <div class="container">
             <ol>
                <li><a href="<%= root_path %>">Trang chủ</a></li>
                <li class="active"><%= @achievements.tag_vn if !@achievements.nil? %></li>
             </ol>
          </div>
       </div>
       <!-- /Breadcrumbs -->
       <section class="section padding-small">
          <div class="container">
             <div class="row">
                <div class="col-sm-6">
                   <h3 class="title text-uppercase"><%= @achievements.title_vn if !@achievements.nil? %></h3>
                   <%= @achievements.content_vn.html_safe if !@achievements.nil? %>
                </div>
                <div class="col-sm-6"> <img class="shadow" src="/frontend/images/demo/our-offices.jpg" alt="Our offices"> </div>
             </div>
          </div>
       </section>
       <section class="section no-top-padding">
          <div class="container">
             <div class="row">
                <div class="col-md-12">
                   <h3 class="title text-uppercase">Hình ảnh - Thành tích</h3> </div>
             </div>
             <div class="row">
                <div class="col-md-12 page-content padding-bottom-xl">
                     <div class="gallery row">
                        <div class="col-xs-2">
                           <figure class="gallery-item">
                              <div class="gallery-icon">
                                 <a href="/frontend/images/thanh-tich-01.jpg"><img src="/frontend/images/thanh-tich-01.jpg" alt="Gallery 1"></a>
                              </div>
                           </figure>
                        </div>
                        <div class="col-xs-2">
                           <figure class="gallery-item">
                              <div class="gallery-icon">
                                 <a href="/frontend/images/thanh-tich-02.jpg"><img src="/frontend/images/thanh-tich-02.jpg" alt="Gallery 2"></a>
                              </div>
                           </figure>
                        </div>
                        <div class="col-xs-2">
                           <figure class="gallery-item">
                              <div class="gallery-icon">
                                 <a href="/frontend/images/thanh-tich-03.jpg"><img src="/frontend/images/thanh-tich-03.jpg" alt="Gallery 3"></a>
                              </div>
                           </figure>
                        </div>
                        <div class="col-xs-2">
                           <figure class="gallery-item">
                              <div class="gallery-icon">
                                 <a href="/frontend/images/thanh-tich-04.jpg"><img src="/frontend/images/thanh-tich-04.jpg" alt="Gallery 4"></a>
                              </div>
                           </figure>
                        </div>
                        <div class="col-xs-2">
                           <figure class="gallery-item">
                              <div class="gallery-icon">
                                 <a href="/frontend/images/thanh-tich-05.jpg"><img src="/frontend/images/thanh-tich-05.jpg" alt="Gallery 4"></a>
                              </div>
                           </figure>
                        </div>
                        <div class="col-xs-2">
                           <figure class="gallery-item">
                              <div class="gallery-icon">
                                 <a href="/frontend/images/thanh-tich-06.jpg"><img src="#" alt="Gallery 4"></a>
                              </div>
                           </figure>
                        </div>
                     </div>
                </div>
             </div>
          </div>
       </section>
    </main>
    <!-- Site Footer -->
    <footer class="site-footer">
       <%= render 'home/footer' %>
    </footer>
    <!-- /Site Footer -->
  </div>
  <!-- Scripts -->
  <%= javascript_include_tag '/frontend/js/jquery.min.js' %>
  <%= javascript_include_tag '/frontend/js/bootstrap/bootstrap.min.js' %>
  <%= javascript_include_tag '/frontend/js/doubletaptogo.js' %>
  <%= javascript_include_tag '/frontend/js/owlcarousel/owl.carousel.min.js' %>
  <%= javascript_include_tag '/frontend/js/functions.js' %>
  <%= javascript_include_tag '/frontend/js/isotope.min.js' %>
  <%= javascript_include_tag '/frontend/js/jquery.swipebox.js' %>
  <%= javascript_include_tag '/frontend/js/waypoints/jquery.waypoints.min.js' %>
  <!-- /Scripts -->
  <!-- Revolution Slider -->
  <%= javascript_include_tag '/frontend/revolution/js/jquery.themepunch.tools.min.js?rev=5.0' %>
  <%= javascript_include_tag '/frontend/revolution/js/jquery.themepunch.revolution.min.js?rev=5.0' %>
  <!-- /Revolution Slider -->